        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (global.flag_boss4 &gt;= 5)
{
    var mindist,maxdist,itr,i,op_num,wid;
    shoot=12; //шагов до повторной стрельбы
    with instance_create(x,y,obj_laser_boss4) 
    {
        audio_play_sound(shot_boss_1,0,false)
        dir=180; //вперед dir=0 назад dir=180
        //оптимальное вычисление точного расстояния до препятствия с минимальным к-вом итераций
        mindist=0; //начальное расстояние
        //maxdist=view_wview; //конечное
        maxdist=x;
        itr=maxdist; //прибавление
        op_num=0; //к-во операций (для отладки и для выхода из цикла если на всей дистанции свободно)
        while(itr&gt;1)//пока прибавление больше одного пикселя 
        {
            for(i=mindist;i&lt;maxdist;i+=itr) 
            {
                op_num+=1; //считаем к-во операций
                wid=collision_line(x+lengthdir_x(i,dir),y+lengthdir_y(i,dir),x+lengthdir_x(i+itr,dir),y+lengthdir_y(i+itr,dir),obj_space_ship,1,0); //wall id
                if wid!=noone 
                { //есть столкновение с кемто
                    wall=wid;
                    mindist=i; //мин дистанция
                    maxdist=i+itr; //макс
                    itr/=2; //делим прибавление на 2 (таким образом в след витке while в for будет 2 итерации)
                    break; //прервать for
                }
                else 
                {
                    if op_num=1//если первая операция, значит по всей длине препятствий нет
                    {
                        wall=noone; //стен нет
                        itr=0; //чтобы больше не заводился цикл while
                        break; //прервать for
                    }
                }
            }
        }
        dist=maxdist;
        if wall!=noone 
        {
            lives-=1;
            effect_create_above(ef_spark,x+lengthdir_x(dist,dir),y+lengthdir_y(dist,dir),2,c_red);
            effect_create_above(ef_firework,x+lengthdir_x(dist,dir),y+lengthdir_y(dist,dir),1,c_orange);
            effect_create_above(ef_smokeup,x+lengthdir_x(dist,dir),y+lengthdir_y(dist,dir),2,c_gray);
            //effect_create_above(kind,x,y,size,color) 0 – маленький, 1 – средний, 2 – большой.
            audio_play_sound(hit_the_player,0,false)
        }
    }
}
</string>
