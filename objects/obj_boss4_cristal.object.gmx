            <string>x = obj_boss4.x-70
y = obj_boss4.y-160

// Формала перевода градусов в радианы для вычисления
phi_rad = PHI * i * pi / 180.0
x += sin( phi_rad ) * RAD
y += cos( phi_rad ) * RAD + 60
