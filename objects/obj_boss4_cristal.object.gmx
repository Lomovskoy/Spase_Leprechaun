            <kind>1</kind>
            <string>if (global.shining_crystal == true)
    {
        draw_sprite(spr_boss4_2,-1,x,y)
        boss4_step(global.tine_crystal,global.shining_crystal)
    }
else if (global.shining_crystal == false)
